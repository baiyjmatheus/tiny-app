<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% include partials/_header %>

    <ul>
      <% for (let url in urls) { %>
        <li><%= url %>: <a href="<%= urls[url] %>"><%= urls[url] %></a></li>
        <!-- Delete Button -->
        <form action="/urls/<%= url %>/delete" method="POST">
          <input type="submit" value="Delete">
        </form>
        <!-- Edit Button -->
        <a href="/urls/<%= url %>">Edit</a>
      <% } %>
    </ul>
  </body>
  
</html>