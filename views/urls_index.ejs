<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% include partials/_header %>

    <ul>
      <% for (let url in urls) { %>
        <li><%= urls[url] %>: <%= url %></li>
        <form action="/urls/<%= url %>/delete" method="POST">
          <input type="submit" value="Delete">
        </form>
      <% } %>
    </ul>
  </body>
  
</html>